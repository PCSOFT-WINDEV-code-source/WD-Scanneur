        code : |1-
         SI Libellé_Scanner_En_Cours = CS_AUCUN_SCANNEUR ALORS	
         	BTN_Choix_Périphérique.ExécuteTraitement(trtClic)
         FIN
         
         
         // Numérisation du document
         SI Numérisation() = Faux ALORS
         	Erreur("Une erreur est apparue pendant la numérisation : ",ErreurInfo())
         	RETOUR
         FIN
         
         // On affiche un toast
         ToastAffiche("Numérisation terminée",toastCourt,cvMilieu,chCentre)
         
         
         SELON COMBO_TYPE_NUMERISATION
         	CAS cnDestinationFichierBMP, cnDestinationFichierGIF, cnDestinationFichierJPG
         		MaFenêtre..Plan = IMAGE
         		EI_APERCU = SAI_FICHIER_DESTINATION
         	cas cnDestinationFichierPDF	
         		MaFenêtre..PLan = PDF
         		PDF_AFFICHAGE = SAI_FICHIER_DESTINATION
         		
         	AUTRE CAS
         		
         FIN
        type : 18
