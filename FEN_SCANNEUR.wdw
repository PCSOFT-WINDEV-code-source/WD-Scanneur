     internal_properties : CAAAAAgAAACXYXa9LasJEmuw277gq2ipJ7FU4IjxE9+b1vcsBEHXPwWwJvQjLAExHM/fIDyKDe47jtKMoLjkBgRbCp+2CvgSi0PGNVMesoKqrESeLPzCCFn7yu3QVXAB9r6H3sWjEcKzh/GD4HjeVhi6azA=
     code : |1+
      GLOBAL
      	gfsNomImageTemp est une chaîne =""					// Nom du ficher image temporaire (utilisée pour l'impression)
      	gMonScanneur est un Scanneur						// Accès au scanneur
      
      CONSTANT
      	CS_AUCUN_SCANNEUR =  "<aucun scanneur sélectionné>"
      	CS_MEMORISATION_SCANNEUR = "NOM_SCANNEUR"
      FIN
      	
      // Nom du fichier image temporaire
      gfsNomImageTemp = ComplèteRep(fRepEnCours())+"temp.jpg"
      
      // Plan par défaut de la fenêtre
      MaFenetre..plan = DEFAUT
      
      CONSTANTE
      	// Destination de la numérisation
      	cnDestinationFichierPDF 	= 1
      	cnDestinationFichierJPG 	= 2	
      	cnDestinationFichierBMP 	= 3
      	cnDestinationFichierGIF 	= 4
      	
      	// Volets du champ disposition	
      	cnVoletCouleurs			= 5
      	cnVoletQualité			= 6	
      	cnVoletModeCouleur		= 7
      	cnVoletResolution		= 8
      FIN
      
   -
     code : |1-
      SI ChargeParamètre(CS_MEMORISATION_SCANNEUR)<>"" alors
      	gMonScanneur = ScanRécupère(ChargeParamètre(CS_MEMORISATION_SCANNEUR))
      	// Est-ce que le scanneur est toujours valide ?
      	SI gMonScanneur.Nom <> "" alors
      		// On utilise ce scanneur
      		Libellé_Scanner_En_Cours = gMonScanneur.Nom		
      	FIN
      FIN
