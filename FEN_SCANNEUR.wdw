  procedures :
   -
     name : Numérisation
     procedure_id : 1269193386105684622
     type_code : 14
     code : |1-
      Procédure Numérisation():booléen
      
      // Variables locales
      MonImage est une image
      
      // Prise en compte des paramètres définis par l'utilisateur
      GestionParamètresNumérisation()
      
      // Le format d'acquisition change en fonction de l'onglet
      SELON COMBO_TYPE_NUMERISATION
      	CAS cnDestinationFichierPDF			// PDF
      		ScanVersPDF(gMonScanneur,scanNombrePagesToutes,SAI_FICHIER_DESTINATION)
      						
      	CAS cnDestinationFichierBMP : 		// Bitmap		
      		MonImage = ScanVersImage(gMonScanneur)
      		MonImage.SauveBMP(SAI_FICHIER_DESTINATION)
      		
      	CAS cnDestinationFichierGIF : 		// GIF			
      		MonImage = ScanVersImage(gMonScanneur)
      		MonImage.SauveGIF(SAI_FICHIER_DESTINATION,COMBO_NOMBRE_COULEURS..ValeurAffichée)
      
      	CAS cnDestinationFichierJPG : 		// JPEG
      		MonImage = ScanVersImage(gMonScanneur)
      		MonImage.SauveJPEG(SAI_FICHIER_DESTINATION,Val(COMBO_QUALITE))			
      FIN
      
      // Affichage du résultat
      IMG_PREVISUALISATION = 	SAI_FICHIER_DESTINATION
      
      // Gestion des erreurs
      Si ErreurDétectée alors	
      	RENVOYER Faux
      FIN
      
      // Par défaut, la numérisation se déroule correctement
      RENVOYER Vrai
     type : 458752
