      PROCÉDURE AfficheOptionsSelonDestination() <IHM>
      
      // On affiche les volets en fonction de la destination
      SELON COMBO_TYPE_NUMERISATION
      	CAS cnDestinationFichierPDF
      		DISP_OPTIONS[cnVoletQualité]..VisibleAvecAnimation	= Faux
      		DISP_OPTIONS[cnVoletCouleurs]..VisibleAvecAnimation	= Faux		
      	CAS cnDestinationFichierBMP
      		DISP_OPTIONS[cnVoletQualité]..VisibleAvecAnimation	= Faux
      		DISP_OPTIONS[cnVoletCouleurs]..VisibleAvecAnimation	= Faux				
      	CAS cnDestinationFichierGIF
      		DISP_OPTIONS[cnVoletCouleurs]..VisibleAvecAnimation = Vrai
      		DISP_OPTIONS[cnVoletQualité]..VisibleAvecAnimation	= Faux
      	CAS cnDestinationFichierJPG
      		DISP_OPTIONS[cnVoletQualité]..VisibleAvecAnimation = Vrai
      		DISP_OPTIONS[cnVoletCouleurs]..VisibleAvecAnimation	= Faux
      FIN
      
