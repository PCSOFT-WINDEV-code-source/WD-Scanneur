      PROCÉDURE sExtensionSelonDestination(sNomFichier):chaîne
      
      SELON COMBO_TYPE_NUMERISATION
      	CAS cnDestinationFichierBMP
      		RENVOYER fExtraitChemin(sNomFichier,fDisque+fFichier+fRépertoire)+".bmp"
      	CAS cnDestinationFichierGIF
      		RENVOYER fExtraitChemin(sNomFichier,fDisque+fFichier+fRépertoire)+".gif"
      	CAS cnDestinationFichierJPG
      		RENVOYER fExtraitChemin(sNomFichier,fDisque+fFichier+fRépertoire)+".jpg"
      	CAS cnDestinationFichierPDF
      		RENVOYER fExtraitChemin(sNomFichier,fDisque+fFichier+fRépertoire)+".pdf"
      	AUTRE CAS
      		RENVOYER sNomFichier
      FIN
