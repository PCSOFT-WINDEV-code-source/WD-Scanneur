     code : |1-
      Procédure GestionParamètresNumérisation()
      
      // Résolution de l'image
      gMonScanneur.Résolution = Combo_Résolution
      
      // Source du scanneur
      SELON COMBO_SOURCE.Select()
      	CAS 1
      		gMonScanneur.ScanDepuis = scanDepuisAuto
      	cas 2
      		gMonScanneur.ScanDepuis = scanDepuisChargeur
      	cas 3		
      		gMonScanneur.ScanDepuis = scanDepuisPlateau	
      FIN
      
      // Format de l'image
      SELON Combo_Format.select()
      	CAS 1 : // Selon périphérique
      		gMonScanneur.FormatCouleur = scanFormatCouleurAuto 
      	CAS 2 : // Mode RVB
      		gMonScanneur.FormatCouleur = scanFormatCouleurRVB  
      	CAS 3 : // Niveaux de gris
      		gMonScanneur.FormatCouleur = scanFormatCouleurNiveauGris   
      	CAS 4 : // Noir et Blanc
      		gMonScanneur.FormatCouleur = scanFormatCouleurNoirBlanc    
      
      FIN
      
